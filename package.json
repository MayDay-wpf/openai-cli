{
  "name": "openai-cli-unofficial",
  "version": "0.0.5",
  "description": "A powerful OpenAI CLI Coding Agent built with TypeScript",
  "main": "dist/index.js",
  "bin": {
    "openai-cli": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "start:no-warnings": "node --no-deprecation dist/index.js",
    "dev": "cross-env NODE_OPTIONS=\"--no-deprecation\" ts-node src/index.ts",
    "dev:original": "ts-node src/index.ts",
    "dev:quiet": "cross-env NODE_NO_WARNINGS=1 ts-node src/index.ts",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "prepare": "npm run build"
  },
  "keywords": [
    "openai",
    "cli",
    "coding",
    "agent",
    "typescript",
    "ai",
    "assistant",
    "programming"
  ],
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/MayDay-wpf/openai-cli#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MayDay-wpf/openai-cli.git"
  },
  "bugs": {
    "url": "https://github.com/MayDay-wpf/openai-cli/issues"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "preferGlobal": true,
  "dependencies": {
    "@inquirer/prompts": "^7.6.0",
    "boxen": "^8.0.1",
    "chalk": "^4.1.2",
    "cli-highlight": "^2.1.11",
    "commander": "^11.1.0",
    "figlet": "^1.7.0",
    "highlight.js": "^11.11.1",
    "marked": "^12.0.2",
    "mcp-client": "^1.13.0",
    "openai": "^4.62.1",
    "tiktoken": "^1.0.21"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.8",
    "@types/inquirer": "^9.0.8",
    "@types/marked": "^5.0.2",
    "@types/node": "^20.19.4",
    "cross-env": "^7.0.3",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.0"
  }
}
